
#include <stdio.h>
#include <conio.h>

int main()

{
///integer
    
//Example of int

  int a;
  
  a = 24;
  
  printf("%d", a);
  
  printf("\n");
  
//Example of Short int

  short int b;
  
  b=12345;

  printf("%d" , b);

  printf("\n");

//Example of Long int 

 long int c;
 
 c=-21224775;

 printf("%ld" , c);

 printf("\n");
 
 //Example of unsigned int
 
 unsigned int d;
 
 d = 99999999;
 
 printf("%u", d);
 
 printf("\n");
 
 //Example of unsigned short int
 
 unsigned short int e;
 
 e = 22222;
 
 printf("%hu", e);
 
 printf("\n");
 
//Example of unsigned long int

 unsigned long int f;
 
 f = 99999999;
 
 printf("%lu", f);
 
 printf("\n");
 
 
 ///Character 
 
//Example of char (for print Character)
    
  char g;
  
  g = 'a';

  printf("%c", g);

  printf("\n");
    

//Example of char (for print Character Asking Value) 

 char h;
 
 h = 'a';

 printf("%d", h);

 printf("\n");

//Example of char (for print Character with Asking Value)

 char i;
 
 i = 96;

 printf("%d", i);

 printf("\n");

//Example of signed char

 signed char j;
 
 j = 79;
 
 printf("%c", j);
 
 printf("\n");
 
//Example of unsigned char

 unsigned char k;
 
 k = 78;
 
 printf("%c", k);
 
 printf("\n");
 
 
///Floating Point
 
//Example of float 
    
 float l;
 
 l = 3.121314;

 printf("%f", l);

 printf("\n");

//Example of double 

 double m;
 
 m = 2.101112131415161;

 printf("%.15lf", m);

 printf("\n");

//Example of long double (it Can't Works in any device)

 long double n;
 
 n = 4.101112131415161718;

 printf("%.15Lf", n);


    getch();
    return 0;
}
