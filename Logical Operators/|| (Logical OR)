#include<stdio.h>
#include<conio.h>

int main() {

int age,marks;

printf("Enter your age:::");

scanf("%d", &age);

printf("Enter your marks:::");

scanf("%d", &marks);


if(age >21 || marks >33){
    
    printf("Thank you for Registration");
    
}
    else{
        
        printf("Sorry you are Not eligible for Registration");
        
}
    

getch();
return 0;

}
