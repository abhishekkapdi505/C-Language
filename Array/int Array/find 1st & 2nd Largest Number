#include <stdio.h>
#include <conio.h>
#include <string.h>
#include <math.h>

int main () {

 int a[20];
 int i,j,large,slarge,size;
 
 printf("Enter a Size::");
 scanf("%d", &size);



 for(i = 0; i < size; i++) {
 printf("Enter a Number::");
 scanf("%d", &a[i]);

 large = a[0];
 slarge = a[0];
  
 }

 for(i = 0; i < size; i++) {
 printf("\t %d", a[i]);

 if(a[i] > large) {
 large = a[i];
 
}
 }

for(i = 0; i < size; i++) {
  if(a[i] > slarge && a[i] != large) {
   slarge = a[i];

}
}
 
 
printf("\nlarge Number is::%d",large);
printf("\nSecond largest Number::%d",slarge);

 getch();
 return 0;

}
