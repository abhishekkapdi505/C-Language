#include <stdio.h>
#include <conio.h>

 int sum();

 void main() {
 
int a,b,result;

printf("Enter a Value of a::");
 scanf("%d", &a);

 printf("Enter a Value of b::");
 scanf("%d", &b);

 result = sum(a,b);

 printf("Sum is::%d", result);

 getch();

}

 int sum(int x,int y) {
 
int c;

  c = x + y;

 getch();
 return (c);

}
